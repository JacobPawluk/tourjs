{{#if colSelected}}
  {{battleship-move-selector-bike-cursor 
      name="Select Target Row" 
      actionsList=rowActions 
      onSelectAction=(action 'onSelectRow') 
      onPendingSelection=(action 'onPendingRow')
      tmNextEvaluation=tmNextEvaluation}}
{{else}}
  {{battleship-move-selector-bike-cursor 
      name="Select Target Column" 
      actionsList=colActions 
      onSelectAction=(action 'onSelectCol') 
      onPendingSelection=(action 'onPendingCol')
      tmNextEvaluation=tmNextEvaluation}}
{{/if}}